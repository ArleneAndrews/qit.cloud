{
  "name": "website-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "mirrorx": "^0.2.11",
    "mousetrap": "^1.6.2",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-scripts": "1.1.4",
    "react-sortable-hoc": "^0.8.3",
    "styled-components": "^3.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build; node updateBuildCacheVersion.js; cp _redirects build/.",
    "testBuild": "react-scripts build",
    "serve": "serve build -l 3000",
    "serveDave": "serve --no-clipboard --single build -l 3000",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:record": "cypress:run --record",
    "local:e2e": "start-server-and-test start http://localhost:3000 cypress:run",
    "ci:e2e": "start-server-and-test start http://localhost:3000 cypress:run --record",
    "lint": "standard --fix",
    "precommit": "standard",
    "concurrentlyScripts": "concurrently npm:test npm:ci:e2e --kill-others-on-fail"
  },
  "devDependencies": {
    "serve": "~10.0.2",
    "cypress": "^3.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^23.1.0",
    "react-test-renderer": "^16.4.1",
    "sinon": "^6.0.0",
    "standard": "^11.0.1",
    "start-server-and-test": "=1.7.11",
    "concurrently": "^4.0.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "/build"
    ],
    "globals": [
      "cy",
      "Cypress"
    ],
    "env": {
      "jest": true
    }
  }
}
